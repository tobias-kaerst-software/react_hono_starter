{
  "files.exclude": {
    "**/deno.lock": true
  },

  "eslint.enable": false,
  "deno.enable": true,
  "deno.lint": true,

  "[json][jsonc][html][javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "denoland.vscode-deno"
  }
}
